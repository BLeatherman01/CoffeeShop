@model IEnumerable<Product>
<h1> Product Details</h1>

<form asp-action="ProductList" asp-controller="Home">
    @foreach (Product c in Model)
    {
        <div>
            <p>@c.ProductName</p>
            <p>@c.Category</p>
            <p>@c.Description</p>
            <p>@c.Price</p>
        </div>
        <br />
    }
</form>
